{
  "name": "better-tabletop",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "fuse.js": "^3.3.0",
    "jsonwebtoken": "^8.5.0",
    "mongoose": "^5.2.4",
    "morgan": "^1.9.0",
    "nodemon": "^1.18.3",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "styled-components": "^4.1.3",
    "universal-cookie": "^3.1.0"
  },
  "scripts": {
    "start": "nodemon server/index.js --watch server --exec babel-node --presets env",
    "watch": "react-scripts start",
    "watch_DEVMODE": "yarn dev_mode_true && react-scripts start",
    "dev_mode_true": "cp ./src/helpers/dev_mode_true.js ./src/config.js",
    "dev_mode_false": "cp ./src/helpers/dev_mode_false.js ./src/config.js",
    "build": "react-scripts build",
    "build_DEVMODE": "yarn dev_mode_false && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {},
  "nodemonConfig": {
    "ignore": [
      "server/data/*.json",
      "server/data/projects/*.json"
    ]
  }
}
